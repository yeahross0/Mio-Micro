	'sunk_putt',
	'whistle',
	'frying_pan',
	'bell',
	'knife_chop',
	'mobile_phone',
	'razor',
	'mobile_phone',
	'popped_cork',
	'water',
	'sneeze',
	'snap',
	'munching',
	'gulp',
	'punch',
	'foot_stamp',
	'gasp',
	'applause',
	'cat',
	'big_dog',
	'pig',
	'small_dog',
	'wolf',
